@prefix : <https://github.com/LuSoMaBra/semantic-esp/tree/master/semanticesp/ontology/ontologia_psesp.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .
@prefix psesp: <https://github.com/LuSoMaBra/semantic-esp/tree/master/semanticesp/ontology/ontologia_psesp.owl#> .

<https://github.com/LuSoMaBra/semantic-esp/tree/master/semanticesp/ontologia_psesp> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

psesp:isPropertyOff a rdf:Property, owl:ObjectProperty ;
	rdfs:domain
                schema:Course,
                schema:CollegeOrUniversity,
               dcterms:ProvenanceStatement,
                 psesp:Trabalho ;
    rdfs:range
                schema:name ,
                schema:description ,
                schema:courseMode ,
                schema:jobTitle ,
                schema:name ,
                schema:branchCode ,
                schema:url ,
                schema:streetAddress ,
                schema:addressLocality ,
                schema:addressRegion ,
                schema:postalCode ,
                schema:title ,
               dcterms:creator ,
               dcterms:created ,
               dcterms:modified ,
                 psesp:AreaCNAEF ,
                 psesp:AreaCurso ,
                 psesp:DuracaoCurso ,
                 psesp:NivelDeFormacao ,
                 psesp:ValorPropinaInternacional ,
                 psesp:ValorPropinaNacional ,
                 psesp:RequisitosTrabalho ,
                 psesp:RemuneracaoTrabalho ,
                 psesp:ModoTrabalho ,
                 psesp:lastExtraction ,
                 psesp:source ;
    rdfs:label "é propriedade de"@pt .

dcterms:isPartOf a rdf:Property, owl:ObjectProperty ;
	rdfs:domain schema:CollegeOrUniversity ;
	rdfs:range schema:Course  ;
    rdfs:label "é parte de"@pt .

dcterms:hasPart a rdf:Property, owl:ObjectProperty ;
	rdfs:domain dcterms:ProvenanceStatement ;
	rdfs:range schema:CollegeOrUniversity ,
	           schema:Course ,
	           psesp:Trabalho ;
    rdfs:label "tem parte de"@pt .

schema:sameAs a rdf:Property, owl:ObjectProperty ;
	rdfs:domain psesp:RequisitosTrabalho ;
	rdfs:range psesp:AreaCurso ;
    rdfs:label "é igual a"@pt .

#################################################################
#    Data properties
#################################################################

schema:name a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:Course,
	            schema:CollegeOrUniversity ;
	rdfs:range xsd:string ;
	rdfs:label "Área profissional do curso"@pt .

schema:description a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:Course,
	             psesp:Trabalho ;
	rdfs:range xsd:string ;
	rdfs:label "Descrição"@pt .

schema:courseMode a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:Course ;
	rdfs:range xsd:string ;
	rdfs:label "Modo curso (Presencial/Remoto)"@pt .

schema:jobTitle a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain psesp:Trabalho ;
	rdfs:range xsd:string ;
    rdfs:label "Título da oferta de emprego"@pt .

schema:name a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:CollegeOrUniversity ,
	            schema:Course ;
	rdfs:range xsd:string ;
	rdfs:label "Nome da Universidade ou Instituto de ensino superior"@pt .

schema:branchCode a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:CollegeOrUniversity ;
	rdfs:range xsd:string ;
	rdfs:label "Código da Universidade ou Instituto de ensino superior"@pt .

schema:url a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:CollegeOrUniversity ,
               dcterms:ProvenanceStatement ,
	            schema:Course ;
	rdfs:range xsd:string ;
	rdfs:label "URL de origem da informação"@pt .

schema:streetAddress a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:CollegeOrUniversity ;
	rdfs:range xsd:string ;
	rdfs:label "Morada"@pt .

schema:addressLocality a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:PostalAddress ;
	rdfs:range xsd:string ;
	rdfs:label "Concelho"@pt .

schema:addressRegion a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:CollegeOrUniversity ;
	rdfs:range xsd:string ;
	rdfs:label "Distrito"@pt .

schema:postalCode a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:CollegeOrUniversity ;
	rdfs:range xsd:string ;
	rdfs:label "Código postal"@pt .

schema:title a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain dcterms:ProvenanceStatement ;
	rdfs:range xsd:string ;
    rdfs:label "Nome do provedor da informação"@pt .

dcterms:creator a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain dcterms:ProvenanceStatement ;
	rdfs:range xsd:string ;
    rdfs:label "Nome do criador da informação"@pt .

dcterms:created a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain dcterms:ProvenanceStatement ;
	rdfs:range xsd:datetime ;
    rdfs:label "Data e hora da criação da informação"@pt .

dcterms:modified a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain dcterms:ProvenanceStatement ;
	rdfs:range xsd:datetime ;
    rdfs:label "Data e hora da última modificação da informação"@pt .

psesp:AreaCNAEF a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:Course ;
	rdfs:range xsd:string ;
	rdfs:label "Área profissional do curso"@pt .

psesp:AreaCurso a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:Course ;
	rdfs:range xsd:string ;
	rdfs:label "Área de estudo do curso"@pt .

psesp:DuracaoCurso a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:Course ;
	rdfs:range xsd:string ;
	rdfs:label "Duração do curso (anos)"@pt .

psesp:NivelDeFormacao a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:Course ;
	rdfs:range xsd:string ;
	rdfs:label "Qualificação do curso"@pt .

psesp:ValorPropinaInternacional a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:CollegeOrUniversity ;
	rdfs:range xsd:string ;
    rdfs:label "Valor da propina para alunos internacionais"@pt .

psesp:ValorPropinaNacional a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain schema:CollegeOrUniversity ;
	rdfs:range xsd:string ;
    rdfs:label "Valor da propina para alunos nacionais"@pt .

psesp:RequisitosTrabalho a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain psesp:Trabalho ;
	rdfs:range xsd:string ;
    rdfs:label "Requisitos acadêmicos para ocupar a vaga de emprego (Área do Curso)"@pt .

psesp:RemuneracaoTrabalho a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain psesp:Trabalho ;
	rdfs:range xsd:string ;
    rdfs:label "Remuneração oferecidade pela oferta de emprego"@pt .

psesp:ModoTrabalho a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain psesp:Trabalho ;
	rdfs:range xsd:string ;
    rdfs:label "Modo de trabalho (remoto/presencial/ambos)"@pt .

psesp:lastExtraction a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain dcterms:ProvenanceStatement ;
	rdfs:range xsd:datetime ;
    rdfs:label "Data e hora da extração da informação"@pt .

psesp:source a rdf:Property, owl:DatatypeProperty ;
	rdfs:domain dcterms:ProvenanceStatement ;
	rdfs:range xsd:string ;
    rdfs:label "Formato da informação"@pt .


#################################################################
#    Classes
#################################################################

schema:Course rdf:type owl:Class ;
    rdfs:label "Curso Superior"@pt .

schema:CollegeOrUniversity rdf:type owl:Class ;
    rdfs:label "Instituição de Curso Superior"@pt .

dcterms:ProvenanceStatement rdf:type owl:Class ;
    rdfs:label "Provedor da Informação"@pt .

psesp:Trabalho rdf:type owl:Class ;
    rdfs:label "Oferta de Trabalho"@pt .